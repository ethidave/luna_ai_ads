# Production Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://api.lunaluna-ai.com
NEXT_PUBLIC_API_VERSION=v1

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=auth.lunaluna-ai.com
NEXT_PUBLIC_AUTH_CLIENT_ID=your_production_client_id

# AI Services
NEXT_PUBLIC_GEMINI_API_KEY=your_production_gemini_key
NEXT_PUBLIC_AI_SERVICE_URL=https://ai.lunaluna-ai.com

# Social Media APIs
NEXT_PUBLIC_FACEBOOK_APP_ID=your_production_facebook_app_id
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_production_google_client_id

# Analytics
NEXT_PUBLIC_GA_TRACKING_ID=your_production_ga_id
NEXT_PUBLIC_GTM_ID=your_production_gtm_id

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_ENABLE_SOCIAL_MEDIA=true

# Performance
NEXT_PUBLIC_ENABLE_ANALYTICS_COLLECTION=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true

# Security
NEXT_PUBLIC_CSP_NONCE=your_production_nonce
NEXT_PUBLIC_ENABLE_SECURITY_HEADERS=true
